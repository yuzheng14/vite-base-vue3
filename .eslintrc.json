{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential",
    // 使用 eslint-plugin-prettier 也无需配置 "prettier"
    // "prettier",
    // eslint-config-prettier v8 开始无需配置其他
    // 只需配置 "prettier" 即可
    // "prettier/vue.js",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-var": "error"
  }
}
